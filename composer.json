{
    "name": "xenomedia/zeus",
    "description": "The best of Drupal, curated by Xeno Media",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drush/drush": "^9.0",
        "drupal/drupal-extension": "^3.1",
        "jakoch/phantomjs-installer": "1.9.8",
        "behat/behat": "^3.0",
        "drupal/coder": "8.*",
        "phing/phing": "^2.14"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "require": {
        "drupal/lightning": "8.1.0",
        "drupal/google_analytics": "8.2.*",
        "composer/installers": "^1.0",
        "drupal/metatag": "8.1.0-beta9"
    },
    "extra": {
        "installer-paths": {
            "core": [
                "drupal/core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ]
        },
        "patches": {
            "drupal/core": {
                "Fixes Uncaught PHP Exception when selecting too many tests to be tested from UI" : "https://www.drupal.org/files/issues/2575725-16.patch"
            }
        }
    }
}
