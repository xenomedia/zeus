{
    "name": "xenomedia/zeus",
    "description": "The best of Drupal, curated by Xeno Media",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drush/drush": "^9.0",
        "drupal/drupal-extension": "^3.1",
        "jakoch/phantomjs-installer": "1.9.8",
        "behat/behat": "^3.0",
        "phing/phing": "^2.14"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "require": {
        "composer/installers": "^1.0",
        "drupal/lightning": "8.1.04",
        "drupal/admin_toolbar": "8.*",
        "drupal/devel": "8.*",
        "drupal/stage_file_proxy": "8.*",
        "drupal/field_formatter": "8.*",
        "drupal/field_group": "8.*",
        "drupal/google_analytics": "8.2.*",
        "drupal/paragraphs": "8.*",
        "solarium/solarium": "^3.6",
        "drupal/reroute_email": "8.*",
        "drupal/coder": "8.*"
    },
    "extra": {
        "installer-paths": {
            "core": [
                "drupal/core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ]
        }
    }
}
