{
    "name": "xenomedia/zeus",
    "description": "The best of Drupal, curated by Xeno Media",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drush/drush": "^9.0",
        "drupal/drupal-extension": "^3.1",
        "jakoch/phantomjs-installer": "1.9.8",
        "behat/behat": "^3.0",
        "phing/phing": "^2.14"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.0",
        "acquia/lightning": "2.0.1",
        "drupal/admin_toolbar": "^1.0",
        "drupal/devel": "^1.0@beta",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/field_formatter": "^1.0",
        "drupal/field_group": "1.0-rc6",
        "drupal/google_analytics": "^2.0",
        "drupal/paragraphs": "^1.0",
        "solarium/solarium": "^3.6",
        "drupal/reroute_email": "^1.0@beta",
        "drupal/coder": "^2.0"
    },
    "extra": {
        "installer-paths": {
            "core": [
                "drupal/core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ]
        }
    }
}
